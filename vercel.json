{
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "installCommand": "npm ci --legacy-peer-deps",
  "framework": null,
  "regions": ["fra1"],
  "routes": [
    {
      "src": "/file-assets/(.*)",
      "dest": "/file-assets/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ]
}
